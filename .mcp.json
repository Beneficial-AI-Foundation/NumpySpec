{
  "mcpServers": {
    "lean-lsp-mcp": {
      "command": "uvx",
      "args": [
        "lean-lsp-mcp"
      ]
    },
    "LeanTool": {
      "command": "poetry",
      "args": [
        "run",
        "python",
        "leanmcp.py"
      ],
      "cwd": ".mcp-tools/LeanTool"
    },
    "lean-explore": {
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/Users/alokbeniwal/lean-explore",
        "leanexplore",
        "mcp",
        "serve",
        "--backend",
        "local"
      ]
    }
  }
}