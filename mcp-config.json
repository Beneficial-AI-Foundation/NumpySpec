{
  "mcpServers": {
    "lean-lsp": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "lean-lsp-mcp"
      ],
      "env": {}
    },
    "leanexploreAPI": {
      "type": "stdio",
      "command": "leanexplore",
      "args": [
        "mcp",
        "serve",
        "--backend",
        "api",
        "--api-key",
        "__your_api_key__"
      ],
      "env": {}
    },
    "leanexploreLocal": {
      "type": "stdio",
      "command": "leanexplore",
      "args": [
        "mcp",
        "serve",
        "--backend",
        "local"
      ],
      "env": {}
    }
  }
}